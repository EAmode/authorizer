  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug test",
      "preLaunchTask": "npm: build:test",
      "timeout": 1000,
      "program": "${workspaceRoot}/node_modules/jasmine/bin/jasmine",
      "cwd": "${workspaceRoot}/dist/test",
      "outFiles": ["${workspaceFolder}/dist/test/**/*.js"],
      "sourceMaps": true,
      "outputCapture": "std",
      "console": "integratedTerminal"
    }
  ]
}
