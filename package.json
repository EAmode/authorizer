{
  "name": "authorizer",
  "version": "1.0.0",
  "description": "Athorizes requests to resources",
  "main": "dist/authorizer.umd.js",
  "module": "dist/authorizer.js",
  "typings": "dist/types/authorizer.d.ts",
  "scripts": {
    "pretest": "npm run build:test",
    "test": "jasmine \"dist/test/spec/**/*.js\"",
    "build:test": "rimraf dist/test && tsc -p tsconfig.spec.json && cpx \"spec/**/*.json\" dist/test/spec",
    "format": "prettier --write **/*.ts",
    "lint": "tslint -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc && rollup -c && rollup -c -o dist/authorizer.umd.js -f umd -n authorizer --sourcemap",
    "start": "tsc-watch --onSuccess \"rollup -c\""
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eascientific/authorizer.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eascientific/authorizer/issues"
  },
  "homepage": "https://github.com/eascientific/authorizer#readme",
  "dependencies": {
    "cytoscape": "^3.2.11"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 80
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.6",
    "@types/ramda": "^0.25.23",
    "cpx": "^1.5.0",
    "jasmine": "^3.1.0",
    "jasmine-core": "^3.1.0",
    "prettier": "^1.12.1",
    "ramda": "^0.25.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.58.1",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rxjs": "^5.5.10"
  }
}
